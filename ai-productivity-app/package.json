    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "framer-motion": "^10.16.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.294.0",
    "mongoose": "^8.0.0",
    "node-telegram-bot-api": "^0.66.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.9.0",
    "react-router-dom": "^6.26.0",
    "socket.io": "^4.7.0",
