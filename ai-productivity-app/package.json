{
  "name": "ai-productivity-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server/server.js",
    "ai-backend": "cd ai-backend && python app.py",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "dev:all": "concurrently \"npm run server\" \"npm run ai-backend\" \"npm run dev\"",
